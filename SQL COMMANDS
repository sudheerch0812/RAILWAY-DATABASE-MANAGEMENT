CREATE TABLE LOCOPILOTS (
PILOT_ID VARCHAR(50) PRIMARY KEY,
PILOT_NAME VARCHAR(255) NOT NULL,
GENDER VARCHAR(200),
AGE VARCHAR(100),
SALARY VARCHAR(100)
);
CREATE TABLE PASSENGER (
PASSENGER_ID INT PRIMARY KEY,
PASSENGER_NAME VARCHAR(50) NOT NULL,
AGE VARCHAR(255) NOT NULL
GENDER VARCHAR(25),PHONE_NO VARCHAR(10) NOT NULL
);
CREATE TABLE PAYEMENT (
PAYEMENT_ID INT PRIMARY KEY,
PAYEMENT_DATE VARCHAR(50) NOT NULL,
AMOUNT VARCHAR(255) NOT NULL
PAYEMENT_TYPE VARCHAR(25)
);
CREATE TABLE RESERVATION (
RESERVATION_ID INT PRIMARY KEY,
PASSENGER_ID DATE NOT NULL,
TRAIN_ID VARCHAR(255),
SEAT_NUMBER INT,
STATUS VARCHAR(50)
);
CREATE TABLE ROUTE (
ROUTE_ID INT PRIMARY KEY,
SOURCE_STATION VARCHAR(50) NOT NULL,
DESTINATION_STATION VARCHAR(50) NOT NULL,
DISTANCE INT NOT NULL,
TRAVEL_TIME VARCHAR(50),
NUMBER_OF_STOPS VARCHAR(50)
14
);
CREATE TABLE SCHEDULE (
SCHEDULE_ID INT PRIMARY KEY,
DEPARTURE_TIME VARCHAR(50) NOT NULL,
ARRIVAL_TIME VARCHAR(255) NOT NULL,
DAYS_OF_WEEK VARCHAR(20)
);
CREATE TABLE STATION (
STATION_ID INT PRIMARY KEY,
STATION_NAME VARCHAR(50) NOT NULL,
CITY VARCHAR(50) NOT NULL,
NO_OF_PLATFORMS INT NOT NULL
);
CREATE TABLE TICKET (
TICKET_ID INT PRIMARY KEY,
PASSENGER_ID VARCHAR(50),
SEAT_NUMBER VARCHAR(255),
PRICE VARCHAR(200),
BOOKING_TIME VARCHAR(100)
);
CREATE TABLE TRAIN (
SOURCE_STATION VARCHAR(50) PRIMARY KEY,
DESTINATION_STATION VARCHAR(50),
TRAIN_NAME VARCHAR(255),
DEPARTURE_TIME VARCHAR(50),
ARRIVAL_TIME VARCHAR(50),
FREQUENT_DAYS VARCHAR(50)
);
