Creating views:
-- View to display locopilots with their details
CREATE VIEW Locopilots_View AS
SELECT * FROM LOCOPILOTS;
-- View to display passengers with their details
CREATE VIEW Passengers_View AS
SELECT * FROM PASSENGER;
-- View to display payments with their details
CREATE VIEW Payments_View AS
SELECT * FROM PAYMENT;
-- View to display reservations with their details
CREATE VIEW Reservations_View AS
SELECT * FROM RESERVATION;
-- View to display routes with their details
CREATE VIEW Routes_View AS
SELECT * FROM ROUTE;
-- View to display schedules with their details
CREATE VIEW Schedules_View AS
SELECT * FROM SCHEDULE;
-- View to display stations with their details
CREATE VIEW Stations_View AS
SELECT * FROM STATION;
-- View to display tickets with their details
CREATE VIEW Tickets_View AS
SELECT * FROM TICKET;
20
-- View to display trains with their details
CREATE VIEW Trains_View AS
SELECT * FROM TRAIN;
CREATE VIEW Administration_View AS
SELECT * FROM ADMINISTRATION;
-- View to display contracts details
CREATE VIEW Contracts_View AS
SELECT * FROM CONTRACTS;
-- View to display employee details
CREATE VIEW Employee_View AS
SELECT * FROM EMPLOYEE;
-- View to display licensing details
CREATE VIEW Licensing_View AS
SELECT * FROM LICENSING;
SELECT *
FROM RESERVATION R
INNER JOIN PASSENGER P ON R.PASSENGER_ID = P.PASSENGER_ID
INNER JOIN TRAIN T ON R.TRAIN_ID = T.TRAIN_ID;
SELECT *
FROM LOCOPILOTS L
JOIN EMPLOYEE E ON L.PILOT_NAME = E.EMPLOYEE_NAME
JOIN CONTRACTS C ON E.POSITION = C.FOOD_CONTRACTS;
SELECT *
FROM RESERVATION R
JOIN PASSENGER P ON R.PASSENGER_ID = P.PASSENGER_ID
JOIN TRAIN T ON R.TRAIN_ID = T.TRAIN_ID;
SELECT *
FROM ROUTE R
JOIN STATION S1 ON R.SOURCE_STATION = S1.STATION_NAME
JOIN STATION S2 ON R.DESTINATION_STATION = S2.STATION_NAME;
SELECT *
FROM RESERVATION R
JOIN PASSENGER P ON R.PASSENGER_ID = P.PASSENGER_ID
JOIN TRAIN T ON R.TRAIN_ID = T.TRAIN_ID;
SELECT *
FROM ROUTE R
JOIN STATION S1 ON R.SOURCE_STATION = S1.STATION_NAME
JOIN STATION S2 ON R.DESTINATION_STATION = S2.STATION_NAME;
