issue_date VARCHAR(15) UNIQUE,
status VARCHAR(50),
CRM VARCHAR(50)
);
CREATE TABLE CONTRACTS (
CIVIL_WORKS INT,
ELECTRICAL_WORKS VARCHAR(100),
FOOD_CONTRACTS VARCHAR(100) UNIQUE,
COMMODITIES VARCHAR(15)
);
CREATE TABLE EMPLOYEE (
EMPLOYEE_ID INT PRIMARY KEY,
EMPLOYEE_NAME VARCHAR(15) UNIQUE,
GENDER VARCHAR(100),
AGE VARCHAR(255),
POSSITION VARCHAR(100),
SALARY VARCHAR(100)
);
CREATE TABLE FOOD (
BREAKFAST INT PRIMARY KEY,
LUNCH VARCHAR(255),
DINNER VARCHAR(50),
SNACKS VARCHAR(25)
);
CREATE TABLE LICENSING (
LICENSE_ID INT PRIMARY KEY,
LICENSE_NAME VARCHAR(255) NOT NULL,
TURNOVER INT,
DATE_OF_REGISTRATION DATE,
NATURE_OF_LICENSE VARCHAR(100)
);
